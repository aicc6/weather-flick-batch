# Weather Flick Batch 환경 변수 설정
# 실제 사용 시에는 이 파일을 .env로 복사하고 실제 값으로 변경하세요

# 데이터베이스 설정
DB_HOST=localhost
DB_USER=your_db_user
DB_PASSWORD=your_secure_password
DB_NAME=weather_travel_db
DB_PORT=3306

# MySQL Root 비밀번호 (Docker 환경)
MYSQL_ROOT_PASSWORD=your_secure_root_password

# API 키 설정
# 단일 키 사용
# KTO_API_KEY=your_kto_api_key_here
# KMA_API_KEY=your_kma_api_key_here

# 다중 키 사용 (쉼표로 구분)
KTO_API_KEY=your_kto_api_key_1,your_kto_api_key_2,your_kto_api_key_3
KMA_API_KEY=your_kma_api_key_1,your_kma_api_key_2

# API 키별 일일 사용 한도 (기본값: 1000)
KTO_API_DAILY_LIMIT=1000
KMA_API_DAILY_LIMIT=1000

# Grafana 관리자 비밀번호
GRAFANA_ADMIN_PASSWORD=your_secure_grafana_password

# Redis 설정 (필요시)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# 기타 설정
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG