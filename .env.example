# Weather Flick Batch 환경 변수 설정
# 실제 사용 시에는 이 파일을 .env로 복사하고 실제 값으로 변경하세요

# 데이터베이스 설정 (PostgreSQL 권장)
DATABASE_URL=postgresql://username:password@localhost:5432/weather_flick
DB_HOST=localhost
DB_USER=your_db_user
DB_PASSWORD=your_secure_password
DB_NAME=weather_flick
DB_PORT=5432

# PostgreSQL 추가 설정 (선택사항)
POSTGRES_POOL_SIZE=20
POSTGRES_MAX_OVERFLOW=10
POSTGRES_POOL_TIMEOUT=30
POSTGRES_POOL_RECYCLE=3600

# API 키 설정
# 단일 키 사용
# KTO_API_KEY=your_kto_api_key_here
# KMA_API_KEY=your_kma_api_key_here

# 다중 키 사용 (쉼표로 구분)
KTO_API_KEY=your_kto_api_key_1,your_kto_api_key_2,your_kto_api_key_3
KMA_API_KEY=your_kma_api_key_1,your_kma_api_key_2

# API 키별 일일 사용 한도 (기본값: 1000)
KTO_API_DAILY_LIMIT=1000
KMA_API_DAILY_LIMIT=1000

# Grafana 관리자 비밀번호
GRAFANA_ADMIN_PASSWORD=your_secure_grafana_password

# Redis 설정 (캐싱용)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
REDIS_DB=0
REDIS_ENABLED=true

# API Base URLs
KTO_API_BASE_URL=http://apis.data.go.kr/B551011/KorService2
KMA_API_BASE_URL=http://apis.data.go.kr/1360000/VilageFcstInfoService_2.0

# 파일 시스템 설정
DATA_BASE_PATH=./data
BACKUP_ENABLED=true
COMPRESSION_ENABLED=true

# 데이터 품질 설정
QUALITY_CHECK_ENABLED=true
AUTO_TRANSFORM_ENABLED=true
QUALITY_THRESHOLD_OVERALL=85.0

# 기타 설정
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO
MAX_CONCURRENT_REQUESTS=5
REQUEST_TIMEOUT_SECONDS=30